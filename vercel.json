{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload/(.*)",
      "methods": ["POST", "OPTIONS"],
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://evasdeleden.com",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Max-Age": "86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}